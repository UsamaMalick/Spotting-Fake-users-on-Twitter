{
  "manifest_version": 2,

  "name": "Spotting Fake users on Twitter",
  "description": "This extension will tell whether the user is fake or not",
  "version": "1.0",
  "icons": {"128": "icon.png"},
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "check.html"
  },
  "background": {
    "scripts": [
       "check.js"
    ]
  },
  "permissions": [
   "activeTab",
   "unlimitedStorage"
   ]
}